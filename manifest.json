{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage", "https://temporarymail.com/ajax/api.php"],
    "background": {
        "scripts": ["jquery-3.6.0.js"],
        "persistent": false
    },

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/logo.png",
            "32": "images/logo.png",
            "48": "images/logo.png",
            "128": "images/logo.png"
        }
    },
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "manifest_version": 2
}